{
  "name": "gatsby-starter-bluenex",
  "private": true,
  "description": "A Gatsby starter project for me.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "hug-dev": "cd ../backend && hug -f hug_api.py -p 8888",
    "gatsby-prod": "gatsby build && gatsby serve",
    "gatsby-prod-prefix": "gatsby build --prefix-paths && gatsby serve --prefix-paths",
    "elasticsearch": "cd ../backend && ./serve_elasticsearch.sh",
    "index-elastic": "cd ../backend && python index_elasticsearch.py",
    "start": "concurrently -n 'hug,gatsby,elasticsearch' -c 'bgBlue,bgMagenta,bgGreen' 'npm run hug-dev' 'gatsby develop' 'npm run elasticsearch'",
    "deploy": "concurrently -n 'hug,gatsby,elasticsearch' -c 'bgBlue,bgMagenta,bgGreen' 'npm run hug-dev' 'npm run gatsby-prod' 'npm run elasticsearch'",
    "format": "prettier --write src/**/*.{js,jsx}",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@reach/router": "^1.3.4",
    "babel-plugin-styled-components": "^1.12.0",
    "faker": "^4.1.0",
    "gatsby": "^2.32.12",
    "gatsby-plugin-google-analytics": "^2.11.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-plugin-typography": "^2.12.0",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-transformer-csv": "^2.10.0",
    "gatsby-transformer-json": "^2.11.0",
    "moment": "^2.29.4",
    "polished": "^4.1.2",
    "prop-types": "^15.7.2",
    "rc-pagination": "^1.21.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-spinners": "^0.6.1",
    "react-typography": "^0.16.19",
    "shortid": "^2.2.16",
    "styled-components": "^5.2.3",
    "typography": "^0.16.19",
    "typography-theme-moraga": "^0.16.19",
    "use-debounce": "^6.0.1"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.23",
    "concurrently": "^5.3.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react-app": "^5.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "gatsby-plugin-eslint": "^2.0.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bluenex/gatsby-starter-bluenex"
  },
  "bugs": {
    "url": "https://github.com/bluenex/gatsby-starter-bluenex/issues"
  }
}
